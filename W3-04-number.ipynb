   "source": [
    "n=int(input(\"輸入一個正整數：\"))\n",
    "sum1=0\n",
    "for i in range(1,n):\n",
    "    if (n%i==0):\n",
    "        sum1=sum1+i\n",
    "if (sum1>n):\n",
    "    print(n,\"is abundant number\")\n",
    "if (sum1<n):\n",
    "    print(n,\"is deficient number\")\n",
    "if (sum1==n):\n",
    "    print(n,\"is perfact number\")"
